<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://genAgentEdit.caption"
        focusComponent="form">
    <data>
        <instance id="genNodeDc"
                  class="ca.ampautomation.ampata.entity.GenNode"
                  fetchPlan="genAgent-fetch-plan-base">
            <loader/>
        </instance>
    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="windowCommitAndClose" caption="msg:///actions.Ok"
                icon="EDITOR_OK"
                primary="true"
                shortcut="${COMMIT_SHORTCUT}"/>
        <action id="windowClose"
                caption="msg:///actions.Close"
                icon="EDITOR_CANCEL"/>
    </actions>
    <dialogMode height="600"
                width="800"/>
    <layout spacing="true" expand="scrollBox">
        <scrollBox id="scrollBox" spacing="true">
            <form id="form" dataContainer="genNodeDc">
                <column width="350px">
                    <textField id="id2Field" property="id2"/>
                    <textField id="id2CalcField" property="id2Calc"/>
                    <checkBox id="id2CmpField" property="id2Cmp"/>
                    <textField id="id2DupField" property="id2Dup"/>
                    <textField id="classNameField" property="className" editable="false"/>
                    <entityPicker id="parent1_IdField" property="parent1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="parent1_Id2Field" property="parent1_Id2"/>
                    <textArea id="ancestors1_Id2Field" property="ancestors1_Id2" rows="5"/>
                    <textField id="sortIdxField" property="sortIdx"/>
                    <entityPicker id="type1_IdField" property="type1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="type1_Id2Field" property="type1_Id2"/>
                    <textField id="instField" property="inst"/>
                    <textField id="name1Field" property="name1"/>
                    <entityPicker id="name1GenPat1_IdField" property="name1GenPat1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="name1GenPat1_Id2Field" property="name1GenPat1_Id2"/>
                    <textField id="name2Field" property="name2"/>
                    <textField id="abrvField" property="abrv"/>
                    <textArea id="desc1Field" property="desc1" rows="5"/>
                    <textArea id="genDocs1_Id2Field" property="genDocVers1_Id2" rows="5"/>
                    <entityPicker id="genDocVer1_IdField" property="genDocVer1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="genDocVer1_Id2Field" property="genDocVer1_Id2"/>
                    <entityPicker id="genFile1_IdField" property="genFile1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="genFile1_Id2Field" property="genFile1_Id2"/>
                    <textArea id="genFile1_URIField" property="genFile1_URI" rows="5"/>
                    <textField id="genTags1_Id2Field" property="genTags1_Id2"/>
                    <entityPicker id="genTag1_IdField" property="genTag1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup"/>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="genTag1_Id2Field" property="genTag1_Id2"/>
                    <textField id="namePrfxField" property="namePrfx"/>
                    <textField id="nameFrstField" property="nameFrst"/>
                    <textField id="nameMidlField" property="nameMidl"/>
                    <textField id="nameLastField" property="nameLast"/>
                    <textField id="nameSufxField" property="nameSufx"/>
                    <entityPicker id="genAgent1_IdField" property="genAgent1_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup">
                                <properties>
                                    <property name="screenId" value="ampata_GenAgent.browse"/>
                                </properties>
                            </action>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="genAgent1_Id2Field" property="genAgent1_Id2"/>
                    <entityPicker id="genAgent2_IdField" property="genAgent2_Id">
                        <actions>
                            <action id="entityLookup" type="entity_lookup">
                                <properties>
                                    <property name="screenId" value="ampata_GenAgent.browse"/>
                                </properties>
                            </action>
                            <action id="entityClear" type="entity_clear"/>
                        </actions>
                    </entityPicker>
                    <textField id="genAgent2_Id2Field" property="genAgent2_Id2"/>
                </column>
            </form>
        </scrollBox>
        <hbox id="editActions" spacing="true">
            <button id="commitAndCloseBtn" action="windowCommitAndClose"/>
            <button id="closeBtn" action="windowClose"/>
        </hbox>
    </layout>
</window>
