            <simplePagination/>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn" action="genNodesTable.create"/>
                <button id="editBtn" action="genNodesTable.edit"/>
                <button id="refreshBtn" action="genNodesTable.refresh"/>
                <button id="removeBtn" action="genNodesTable.remove"/>
            </buttonsPanel>
        </groupTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
