                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'FinTxact']]>
                </query>
            </loader>
        </collection>
        <instance id="finTxactDc"
                  class="ca.ampautomation.ampata.entity.GenNode"
                  fetchPlan="finTxact-fetch-plan-base">
