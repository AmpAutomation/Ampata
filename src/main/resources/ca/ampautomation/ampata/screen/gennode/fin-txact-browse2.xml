        caption="msg://finTxactBrowse2.caption"
        focusComponent="table">
    <data>
        <collection id="finTxactsDc"
                    class="ca.ampautomation.ampata.entity.GenNode"
                    fetchPlan="finTxact-fetch-plan-base">
            <loader id="genNodesDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'FinTxact']]>
                </query>
            </loader>
        </collection>
        <instance id="finTxactDc"
                  class="ca.ampautomation.ampata.entity.GenNode"
                  fetchPlan="finTxact-fetch-plan-base">
            <loader/>
        </instance>
    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="save" icon="OK" caption="msg:///actions.Ok" shortcut="CTRL-ENTER"/>
        <action id="cancel" icon="CANCEL" caption="msg:///actions.Cancel" description="Esc"/>
