                    <properties include=".*"/>
                    <configurations>
                        <configuration id="filterConfig1" default="true">
                            <propertyFilter caption="id2" operation="CONTAINS" property="id2" operationEditable="true" colspan="2"/>
                            <propertyFilter caption="desc1" operation="CONTAINS" property="desc1" operationEditable="true" colspan="2"/>
                            <propertyFilter caption="beg.date1" operation="GREATER_OR_EQUAL" property="beg.date1" operationEditable="true"/>
                            <propertyFilter caption="beg.date1" operation="LESS_OR_EQUAL" property="beg.date1" operationEditable="true"/>
                            <propertyFilter caption="finAcct1" operation="EQUAL" property="finAcct1_Id" operationEditable="true"/>
                            <propertyFilter caption="genChan1" operation="EQUAL" property="genChan1_Id" operationEditable="true" />
                        </configuration>
                    </configurations>
                </filter>
                <groupTable id="table"
                            width="100%"
                            dataContainer="finTxactsDc">
                    <actions>
                        <action id="create" type="create"/>
                        <action id="duplicate"/>
                        <action id="edit" type="edit"/>
                        <action id="refresh" type="refresh"/>
