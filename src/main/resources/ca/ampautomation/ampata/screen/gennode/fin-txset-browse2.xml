<window xmlns="http://jmix.io/schema/ui/window"
        xmlns:c="http://jmix.io/schema/ui/jpql-condition"
        caption="msg://finTxsetBrowse2.caption"
        xmlns:app="http://schemas.company.com/ui.ex1/0.1/app-ui-components.xsd"
        focusComponent="table">
    <data>
        <collection id="finTxsetsDc"
                    class="ca.ampautomation.ampata.entity.GenNode"
                    fetchPlan="finTxset-fetch-plan-base">
            <loader id="finTxsetsDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'FinTxset']]>
                </query>
            </loader>
        </collection>
        <instance id="finTxsetDc"
                  class="ca.ampautomation.ampata.entity.GenNode"
                  fetchPlan="finTxset-fetch-plan-base">
            <loader/>
        </instance>
        <collection id="finTxsetTypesDc" class="ca.ampautomation.ampata.entity.GenNodeType">
            <fetchPlan extends="_instance_name"/>
            <loader id="finTxsetTypesDl">
                <query>
                    <![CDATA[select e from ampata_GenNodeType e where e.className = 'FinTxset' order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="genChansDc" class="ca.ampautomation.ampata.entity.GenNode">
            <fetchPlan extends="_instance_name"/>
            <loader id="genChansDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'GenChan' order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="genDocsDc" class="ca.ampautomation.ampata.entity.GenNode">
            <fetchPlan extends="_instance_name"/>
            <loader id="genDocsDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'GenDocVer' order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="genTagsDc" class="ca.ampautomation.ampata.entity.GenNode">
            <fetchPlan extends="_instance_name"/>
            <loader id="genTagsDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'GenTag' order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="finHowsDc" class="ca.ampautomation.ampata.entity.FinHow">
            <fetchPlan extends="_instance_name"/>
            <loader id="finHowsDl">
                <query>
                    <![CDATA[select e from ampata_FinHow e order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="finWhatsDc" class="ca.ampautomation.ampata.entity.FinWhat">
            <fetchPlan extends="_instance_name"/>
            <loader id="finWhatsDl">
                <query>
                    <![CDATA[select e from ampata_FinWhat e order by e.id2]]>
                </query>
            </loader>
        </collection>

        <collection id="finWhysDc" class="ca.ampautomation.ampata.entity.FinWhy">
            <fetchPlan extends="_instance_name"/>
            <loader id="finWhysDl">
                <query>
                    <![CDATA[select e from ampata_FinWhy e order by e.id2]]>
                </query>
            </loader>
        </collection>

    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="save" icon="OK" caption="msg:///actions.Ok" shortcut="CTRL-ENTER"/>
        <action id="cancel" icon="CANCEL" caption="msg:///actions.Cancel" description="Esc"/>
    </actions>
    <dialogMode height="600" width="800"/>
    <layout>
        <split id="split" height="100%" orientation="vertical" width="100%" pos="60" reversePosition="true">
            <vbox id="lookupBox" expand="table" height="100%" margin="false,true,false,false" spacing="true">
                <filter id="filter" dataLoader="finTxsetsDl">
                    <properties include=".*"/>
                    <configurations>
                        <configuration id="filterConfig1" default="true">
                            <propertyFilter operation="CONTAINS" property="id2" operationEditable="true"/>
                            <propertyFilter operation="CONTAINS" property="name1" operationEditable="true"/>
                        </configuration>
                    </configurations>
                </filter>
                <groupTable id="table"
                            width="100%"
                            dataContainer="finTxsetsDc">
                    <actions>
                        <action id="create" type="create"/>
                        <action id="duplicate"/>
                        <action id="edit" type="edit"/>
                        <action id="refresh" type="refresh"/>
                        <action id="remove" type="remove"/>
                    </actions>
                    <columns>
                        <column id="id2"/>
                        <column id="id2Calc"/>
                        <column id="id2Cmp"/>
                        <column id="id2Dup"/>
                        <column id="className"/>
                        <column id="parent1_Id"/>
                        <column id="parent1_Id2"/>
                        <column id="ancestors1_Id2"/>
                        <column id="sortOrder"/>
                        <column id="type1_Id"/>
                        <column id="type1_Id2"/>
                        <column id="inst"/>
                        <column id="name1"/>
                        <column id="name1Pat1_Id"/>
                        <column id="name1Pat1_Id2"/>
                        <column id="name2"/>
                        <column id="abrv"/>
                        <column id="code"/>
                        <column id="desc1"/>
                        <column id="status"/>
                        <column id="note"/>
                        <column id="genDocVers1_Id2"/>
                        <column id="genDocVer1_Id"/>
                        <column id="genDocVer1_Id2"/>
                        <column id="genDocVer1_Code"/>
                        <column id="genDocVer1_Ver"/>
                        <column id="genDocVer1_Name1"/>
                        <column id="genFile1_Id2"/>
                        <column id="genTags1_Id2"/>
                        <column id="genTag1_Id2"/>
                        <column id="genTag2_Id2"/>
                        <column id="genTag3_Id2"/>
                        <column id="genTag4_Id2"/>
                        <column id="namePrfx"/>
                        <column id="nameFrst"/>
                        <column id="nameMidl"/>
                        <column id="nameLast"/>
                        <column id="nameSufx"/>
                        <column id="genAgent1_Id2"/>
                        <column id="genAgent2_Id2"/>
                        <column id="beg.ts1"/>
                        <column id="beg.date1"/>
                        <column id="beg.date1Yr"/>
                        <column id="beg.date1Qtr"/>
                        <column id="beg.date1Mon"/>
                        <column id="beg.date1Mon2"/>
                        <column id="beg.date1Day"/>
                        <column id="beg.time1"/>
                        <column id="beg.time1Hr"/>
                        <column id="beg.time1Min"/>
                        <column id="end.ts1"/>
                        <column id="end.date1"/>
                        <column id="end.date1Yr"/>
                        <column id="end.date1Qtr"/>
                        <column id="end.date1Mon"/>
                        <column id="end.date1Mon2"/>
                        <column id="end.date1Day"/>
                        <column id="end.time1"/>
                        <column id="end.time1Hr"/>
                        <column id="end.time1Min"/>
                        <column id="genChan1_Id"/>
                        <column id="genChan1_Id2"/>
                        <column id="idX"/>
                        <column id="finHow1_Id"/>
                        <column id="finHow1_Id2"/>
                        <column id="whatText1"/>
                        <column id="finWhat1_Id"/>
                        <column id="finWhat1_Id2"/>
                        <column id="whyText1"/>
                        <column id="finWhy1_Id"/>
                        <column id="finWhy1_Id2"/>
                        <column id="finTxfers1_Id2"/>
                        <column id="finTxacts1_Id2"/>
                        <column id="finTxacts1_FinAccts1_Id2"/>
                    </columns>
                    <simplePagination/>
                    <buttonsPanel id="buttonsPanel"
                                  alwaysVisible="true">
                        <button id="createBtn" action="table.create"/>
                        <button id="duplicateBtn" action="table.duplicate" caption="Duplicate" icon="font-icon:COPY"/>
                        <button id="editBtn" action="table.edit"/>
                        <button id="refreshBtn" action="table.refresh"/>
                        <button id="removeBtn" action="table.remove"/>
                    </buttonsPanel>
                </groupTable>
                <hbox id="lookupActions" spacing="true" visible="false">
                    <button action="lookupSelectAction"/>
                    <button action="lookupCancelAction"/>
                </hbox>
            </vbox>
            <vbox id="editBox" height="100%" margin="false,false,false,true" expand="fieldGroupBox" spacing="true">
                <scrollBox id="fieldGroupBox">
                    <form id="form" dataContainer="finTxsetDc" width="100%" height="100%">
                        <tabSheet width="100%">
                            <tab id="tab_1"
                                 caption="General"
                                 margin="true"
                                 spacing="true">
                                <form id="tab1Form" dataContainer="finTxsetDc" width="100%" height="100%">
                                    <column width="350px">
                                        <app:textFieldWithButton id="id2FieldWithButton" property="id2"/>
                                        <app:textFieldWithButton id="id2CalcFieldWithButton" property="id2Calc" editable="false"/>
                                        <checkBox id="id2CmpField" property="id2Cmp" editable="false"/>
                                        <textField id="id2DupField" property="id2Dup" editable="false"/>
                                        <textField id="classNameField" property="className" enable="false"/>
                                        <entityComboBox id="type1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="finTxsetTypesDc"
                                                        property="type1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="type1_Id2Field" property="type1_Id2" width="100%"/>
                                        <textField id="name1Field" property="name1" width="100%"/>
                                    </column>
                                    <column width="350px">
                                        <textArea id="desc1Field" property="desc1" rowspan="8" width="100%"/>
                                        <entityComboBox id="genChan1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genChansDc"
                                                        property="genChan1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genChan1_Id2Field" property="genChan1_Id2" width="100%"/>
                                    </column>
                                    <column width="350px">
                                        <textField id="idXField" property="idX"/>
                                        <dateField id="BegDate1Field" property="beg.date1" dateFormat="yyyy-MM-dd"/>
                                        <timeField id="BegTime1Field" property="beg.time1" timeFormat="hh:mm"/>
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_2"
                                 caption="Docs, Files, Tags"
                                 margin="true"
                                 spacing="true">
                                <form id="tab2Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                        <textArea id="genDocs1_Id2Field" property="genDocVers1_Id2" rows="4"/>
                                        <entityComboBox id="genDocVer1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genDocsDc"
                                                        property="genDocVer1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genDocVer1_Id2Field" property="genDocVer1_Id2"/>
                                    </column>
                                    <column width="350px">
                                        <entityPicker id="genFile1_IdField" property="genFile1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityPicker>
                                        <textField id="genFile1_Id2Field" property="genFile1_Id2"/>
                                        <textArea id="genFile1_URIField" property="genFile1_URI" rows="4"/>
                                    </column>
                                    <column width="350px">
                                        <textArea id="genTags1_Id2Field" property="genTags1_Id2" rows="4"/>
                                        <entityComboBox id="genTag1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genTagsDc"
                                                        property="genTag1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genTag1_Id2Field" property="genTag1_Id2"/>
                                        <entityComboBox id="genTag2_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genTagsDc"
                                                        property="genTag2_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genTag2_Id2Field" property="genTag2_Id2"/>
                                        <entityComboBox id="genTag3_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genTagsDc"
                                                        property="genTag3_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genTag3_Id2Field" property="genTag3_Id2"/>
                                        <entityComboBox id="genTag4_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="genTagsDc"
                                                        property="genTag4_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="genTag4_Id2Field" property="genTag4_Id2"/>
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_3"
                                 caption="FinTxset, FinTxact"
                                 margin="true"
                                 spacing="true">
                                <form id="tab3Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                        <textArea id="finTxacts1_Id2Field" property="finTxacts1_Id2" rows="4"/>
                                        <textArea id="finTxacts1_FinAccts1_Id2Field" property="finTxacts1_FinAccts1_Id2" rows="4"/>
                                    </column>
                                    <column width="350px">
                                        <textArea id="finTxfers1_Id2Field" property="finTxfers1_Id2" rows="4"/>
                                    </column>
                                    <column width="350px">
                                    </column>

                                </form>
                            </tab>
                            <tab id="tab_4"
                                 caption="Stmt, Dept, Tax"
                                 margin="true"
                                 spacing="true">
                                <form id="tab4Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_5"
                                 caption="Acct, Amt, Fmla"
                                 margin="true"
                                 spacing="true">
                                <form id="tab5Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_6"
                                 caption="How, What, Why"
                                 margin="true"
