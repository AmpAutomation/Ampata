                                <form id="tab6Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                        <entityComboBox id="finHow1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="finHowsDc"
                                                        property="finHow1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="finHow1_Id2Field" property="finHow1_Id2"/>
                                    </column>
                                    <column width="350px">
                                        <textField id="whatText1Field" property="whatText1"/>
                                        <entityComboBox id="finWhat1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="finWhatsDc"
                                                        property="finWhat1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="finWhat1_Id2Field" property="finWhat1_Id2"/>
                                    </column>
                                    <column width="350px">
                                        <textField id="whyText1Field" property="whyText1"/>
                                        <entityComboBox id="finWhy1_IdField" dataContainer="finTxsetDc"
                                                        optionsContainer="finWhysDc"
                                                        property="finWhy1_Id">
                                            <actions>
                                                <action id="entityLookup" type="entity_lookup"/>
                                                <action id="entityClear" type="entity_clear"/>
                                            </actions>
                                        </entityComboBox>
                                        <textField id="finWhy1_Id2Field" property="finWhy1_Id2"/>
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_7"
                                 caption="Note"
                                 margin="true"
                                 spacing="true">
                                <form id="tab7Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="100%">
                                        <textArea id="noteField" property="note"
                                                  height="100%" width="100%"/>
                                    </column>
                                </form>
                            </tab>
                            <tab id="tab_8"
                                 caption="Other"
                                 margin="true"
                                 spacing="true">
                                <form id="tab8Form" dataContainer="finTxsetDc" width="100%">
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                    <column width="350px">
                                    </column>
                                </form>
                            </tab>
                        </tabSheet>
                    </form>
                </scrollBox>
                <hbox id="actionsPane" spacing="true" visible="false">
                    <button id="saveBtn" action="save"/>
                    <button id="cancelBtn" action="cancel"/>
                </hbox>
            </vbox>
        </split>
    </layout>
</window>
