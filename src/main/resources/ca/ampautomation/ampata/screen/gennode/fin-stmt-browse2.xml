<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://finStmtBrowse2.caption"
        focusComponent="table">
    <data>
        <collection id="finStmtsDc"
                    class="ca.ampautomation.ampata.entity.GenNode"
                    fetchPlan="finStmt-fetch-plan-base">
            <loader id="finStmtsDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'FinStmt' ORDER BY e.id2]]>
                </query>
            </loader>
        </collection>
        <instance id="finStmtDc"
                  class="ca.ampautomation.ampata.entity.GenNode"
                  fetchPlan="finStmt-fetch-plan-base">
            <loader/>
        </instance>

        <collection id="finStmtItmsDc"
                    class="ca.ampautomation.ampata.entity.GenNode"
                    fetchPlan="finStmtItm-fetch-plan-base">
            <loader id="finStmtItmsDl">
                <query>
                    <![CDATA[select e from ampata_GenNode e where e.className = 'FinStmtItm' and e.finStmt1_Id = :finStmt1_Id ORDER BY e.id2]]>
                </query>
            </loader>
        </collection>

    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="save" icon="OK" caption="msg:///actions.Ok" shortcut="CTRL-ENTER"/>
        <action id="cancel" icon="CANCEL" caption="msg:///actions.Cancel" description="Esc"/>
    </actions>
