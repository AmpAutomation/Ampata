                <filter id="filter" dataLoader="finStmtsDl" collapsable="true" collapsed="true" columnsCount="4">
                    <properties include=".*"/>
                    <configurations>
                        <configuration id="filterConfig1" default="true">
                            <propertyFilter id="propFilter_id2" caption="id2" operation="CONTAINS" property="id2" operationEditable="true"/>
                            <propertyFilter id="propFilter_desc1" caption="desc1" operation="CONTAINS" property="desc1" operationEditable="true"/>
                            <propertyFilter id="propFilter_idDtDate1GE" caption="idDt.date1" operation="GREATER_OR_EQUAL" property="idDt.date1" operationEditable="true"/>
                            <propertyFilter id="propFilter_idDtDate1LE" caption="idDt.date1" operation="LESS_OR_EQUAL" property="idDt.date1" operationEditable="true"/>
                            <propertyFilter id="propFilter_genChan1_Id" caption="genChan1" operation="EQUAL" property="genChan1_Id" operationEditable="true">
