                            </propertyFilter>
                            <propertyFilter id="propFilter_finAcct1_Id" caption="finAcct1" operation="EQUAL" property="finAcct1_Id" operationEditable="true">
                                <entityComboBox metaClass="ampata_GenNode" width="200%"/>
                            </propertyFilter>
                            <propertyFilter id="filterConfig1A_status" caption="status" operation="CONTAINS" property="status" operationEditable="true"/>
                        </configuration>
                    </configurations>
                </filter>
