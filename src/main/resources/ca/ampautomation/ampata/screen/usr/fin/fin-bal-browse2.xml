        <collection id="finBalsDc"
                    class="ca.ampautomation.ampata.entity.usr.UsrNode"
                    fetchPlan="finBal-fetch-plan-base">
            <loader id="finBalsDl">
                <query>
                    <![CDATA[select e from ampata_UsrNode e where e.className = 'FinBal' ORDER BY e.sortKey]]>
                </query>
            </loader>
        </collection>
        <instance id="finBalDc"
                  class="ca.ampautomation.ampata.entity.usr.UsrNode"
                  fetchPlan="finBal-fetch-plan-base">
            <loader/>
        </instance>
        <collection id="finTxactItmsDc"
                    class="ca.ampautomation.ampata.entity.usr.UsrNode"
                    fetchPlan="finTxactItm-fetch-plan-base">
            <loader id="finTxactItmsDl">
                <query>
                    <![CDATA[select e from ampata_UsrNode e where e.className = 'FinTxactItm' and e.id2 is null ORDER BY e.id2]]>
                </query>
            </loader>
        </collection>

    </data>
