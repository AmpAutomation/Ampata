<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://usrFinAcctBrowse.caption"
        focusComponent="tableMain">
    <data readOnly="true">
        <collection id="colCntnrMain"
                    class="ca.ampautomation.ampata.entity.usr.base.UsrBaseNode"
                    fetchPlan="fetchPlan_UsrFinAcct_Base">
            <loader id="instCntnrMain">
                <query>
                    <![CDATA[select e from enty_UsrFinAcct e where e.className = 'UsrFinAcct']]>
                </query>
            </loader>
        </collection>
    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="lookupSelectAction"
                caption="msg:///actions.Select"
                icon="LOOKUP_OK"
                primary="true"
                shortcut="${COMMIT_SHORTCUT}"/>
        <action id="lookupCancelAction"
                caption="msg:///actions.Cancel"
                icon="LOOKUP_CANCEL"/>
    </actions>
    <dialogMode width="1024" height="768"/>
    <layout expand="tableMain" spacing="true">
        <filter id="filter"
                dataLoader="instCntnrMain"  collapsable="true" collapsed="true" columnsCount="5">
            <properties include=".*"/>
            <configurations>
                <configuration id="filterConfig1" default="true">
                    <groupFilter operation="AND" caption="GroupA">
                        <propertyFilter operation="CONTAINS" property="id2" operationEditable="true"/>
                        <propertyFilter operation="CONTAINS" property="name1" operationEditable="true"/>
                        <propertyFilter id="filterConfig1A_GenAgent1_Id" caption="genAgent1_Id" operation="EQUAL" property="genAgent1_Id" operationEditable="true">
                            <entityComboBox metaClass="enty_UsrGenAgent"/>
                        </propertyFilter>
                    </groupFilter>
                </configuration>
            </configurations>
        </filter>
        <treeTable id="tableMain"
                   width="100%"
                   dataContainer="colCntnrMain"
                   hierarchyProperty="parent1_Id"
                   multiselect="true"
        >
            <actions>
                <action id="create" type="create">
                    <properties>
                        <property name="screenId" value="enty_FinAcct.edit"/>
                    </properties>
                </action>
                <action id="edit" type="edit">
                    <properties>
                        <property name="screenId" value="enty_FinAcct.edit"/>
                    </properties>
                </action>
                <action id="refresh" type="refresh"/>
                <action id="remove" type="remove"/>
            </actions>
            <columns>
                <column id="id2"/>
                <column id="id2Dup"/>
                <column id="parent1_Id"/>
                <column id="sortIdx"/>
                <column id="type1_Id"/>
                <column id="inst1"/>
                <column id="name1"/>
                <column id="name1GenFmla1_Id"/>
                <column id="name2"/>
                <column id="abrv"/>
                <column id="desc1"/>
                <column id="genAgent1_Id"/>
                <column id="finTaxLne1_Id"/>
                <column id="genDocVers1_Id2"/>
                <column id="genDocVer1_Id"/>
                <column id="genFile1_Id"/>
                <column id="genTag1_Id"/>
            </columns>
            <buttonsPanel id="buttonsPanel"
                          alwaysVisible="true">
                <button id="createBtn" action="tableMain.create"/>
                <button id="editBtn" action="tableMain.edit"/>
                <button id="refreshBtn" action="tableMain.refresh"/>
                <button id="reloadListsBtn" caption="Reload Lists" icon="font-icon:LIST"/>
                <button id="removeBtn" action="tableMain.remove"/>
            </buttonsPanel>
        </treeTable>
        <hbox id="lookupActions" spacing="true" visible="false">
            <button action="lookupSelectAction"/>
            <button action="lookupCancelAction"/>
        </hbox>
    </layout>
</window>
