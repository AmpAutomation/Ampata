                    class="ca.ampautomation.ampata.entity.usr.base.UsrNodeBase"
                    fetchPlan="fetchPlan_UsrFinBalSet_Base">
            <loader id="colLoadrMain">
                <query>
                    <![CDATA[select e from enty_UsrNodeBase e where e.className = 'UsrFinBalSet' ORDER BY e.sortKey, e.id2]]>
                </query>
            </loader>
        </collection>
        <instance id="instCntnrMain"
                  class="ca.ampautomation.ampata.entity.usr.base.UsrNodeBase"
                  fetchPlan="fetchPlan_UsrFinBalSet_Base">
            <loader/>
        </instance>
        <collection id="colCntnrFinBal"
                    class="ca.ampautomation.ampata.entity.usr.base.UsrNodeBase"
                    fetchPlan="fetchPlan_UsrFinBal_Base">
            <loader id="colLoadrFinBal">
                <query>
                    <![CDATA[select e from enty_UsrNodeBase e where e.className = 'UsrFinBal' and e.id2 is null ORDER BY e.sortKey]]>
                </query>
            </loader>
        </collection>
        <collection id="colCntnrFinAcct"
                    class="ca.ampautomation.ampata.entity.usr.base.UsrNodeBase"
                    fetchPlan="fetchPlan_UsrFinAcct_Base">
            <loader id="colLoadrFinAcct">
