                <column id="sortIdx"/>
                <column id="sortKey"/>
                <column id="type1_Id"/>
                <column id="type1_Id2"/>
                <column id="inst1"/>
                <column id="name1"/>
                <column id="name1GenFmla1_Id"/>
                <column id="name1GenFmla1_Id2"/>
                <column id="name2"/>
                <column id="abrv"/>
                <column id="code"/>
                <column id="desc1"/>
