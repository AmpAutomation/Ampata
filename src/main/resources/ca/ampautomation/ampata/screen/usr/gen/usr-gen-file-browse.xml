                <column id="genTag2_Id"/>
                <column id="genTag2_Id2"/>
                <column id="genTag3_Id"/>
                <column id="genTag3_Id2"/>
                <column id="genTag4_Id"/>
                <column id="genTag4_Id2"/>
                <column id="beg1.ts1"/>
                <column id="beg1.date1"/>
                <column id="beg1.date1Yr"/>
                <column id="beg1.date1Qtr"/>
                <column id="beg1.date1Mon"/>
                <column id="beg1.date1Mon2"/>
                <column id="beg1.date1Day"/>
                <column id="beg1.time1"/>
                <column id="beg1.time1Hr"/>
                <column id="beg1.time1Min"/>
                <column id="beg2.ts1"/>
