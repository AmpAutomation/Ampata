                                    <entityComboBox id="type1_IdField" dataContainer="instCntnrMain"
                                                    property="type1_Id">
                                        <actions>
                                            <action id="entityOpen" type="entity_open"/>
                                            <action id="entityClear" type="entity_clear"/>
                                        </actions>
                                    </entityComboBox>
                                    <button id="updateType1_IdFieldListBtn" icon="font-icon:LIST" tabIndex="-1"/>
                                </hbox>
                                <hbox caption="inst1" expand="inst1Field" width="100%">
                                    <textField id="inst1Field" dataContainer="instCntnrMain"
                                               property="inst1"/>
                                    <button id="updateInst1FieldBtn" icon="font-icon:REFRESH" tabIndex="-1"/>
                                    <button id="updateInst1FieldFmlaBtn" icon="font-icon:WRENCH" tabIndex="-1"/>
                                </hbox>
                            </vbox>

                            <vbox width="374px">
                                <hbox caption="ts1.elTs" expand="ts1_ElTsFieldSpace" width="100%" >
                                    <dateField id="ts1_ElTsField" dataContainer="instCntnrMain"
                                               property="ts1.elTs" datatype="localDateTime"/>
