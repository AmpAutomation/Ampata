                                                <button id="updateNm1s1Inst1Int3FieldBtn" icon="font-icon:REFRESH" tabIndex="-1"/>
                                            </hbox>
                                        </hbox>
                                    </groupBox>
                                </hbox>
                            </vbox>

                            <vbox width="374px">
                            </vbox>

                        </hbox>

                    </tab>

                    <tab id="tab_9"
                         caption="Note"
                         expand="tab9_hbox1"
                         margin="true">

                        <hbox id="tab9_hbox1" width="100%" spacing="true">
                            <hbox caption="note" expand="noteField" width="2304px" >
                                <textArea id="noteField" dataContainer="instCntnrMain"
