                                        </hbox>
                                        <hbox caption="inst1" expand="nm1s1Inst1Field" width="100%">
                                            <textField id="nm1s1Inst1Field" dataContainer="instCntnrMain"
                                                       property="inst1"/>
                                            <button id="updateNm1s1Inst1FieldBtn" icon="font-icon:REFRESH" tabIndex="-1"/>
