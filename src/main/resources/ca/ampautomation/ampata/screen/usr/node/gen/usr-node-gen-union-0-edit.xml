                                                   property="sortKey"/>
                                        <button id="updateSortKeyFieldBtn" icon="font-icon:REFRESH" tabIndex="-1"/>
                                    </hbox>
                                </hbox>
                            </vbox>

                            <vbox width="374px">
                                <hbox caption="parent1_Id" expand="parent1_IdField" width="100%">
                                    <entityPicker id="parent1_IdField" dataContainer="instCntnrMain"
                                                  property="parent1_Id">
                                        <actions>
                                            <action id="entityLookup" type="entity_lookup">
                                                <properties>
                                                    <property name="openMode" value="THIS_TAB"/>
                                                    <property name="screenClass" value="ca.ampautomation.ampata.screen.usr.node.base.UsrNodeBase0Lookup"/>
                                                </properties>
                                            </action>
                                            <action id="entityOpen" type="entity_open"/>
                                            <action id="entityClear" type="entity_clear"/>
