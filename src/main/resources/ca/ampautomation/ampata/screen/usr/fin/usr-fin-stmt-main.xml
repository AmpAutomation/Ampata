                                            <checkBox id="tmplt_Beg1Ts1FieldChk" align="MIDDLE_RIGHT"/>
                                        </hbox>
                                        <dateField id="tmplt_Beg1Ts1Field" datatype="localDateTime" dateFormat="yyyy-MM-dd"/>
                                    </hbox>
                                    <hbox caption="end1.ts1">
