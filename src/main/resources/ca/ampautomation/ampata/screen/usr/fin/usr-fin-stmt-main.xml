<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://finStmtMain.caption"
        focusComponent="tableMain">
    <data>
        <collection id="colCntnrMain"
                    class="ca.ampautomation.ampata.entity.usr.UsrNode"
                    fetchPlan="fetchPlan_UsrFinStmt_Base">
            <loader id="colLoadrMain">
                <query>
                    <![CDATA[select e from enty_UsrNode e where e.className = 'UsrFinStmt' ORDER BY e.id2]]>
                </query>
            </loader>
        </collection>
        <instance id="instCntnrMain"
                  class="ca.ampautomation.ampata.entity.usr.UsrNode"
                  fetchPlan="fetchPlan_UsrFinStmt_Base">
            <loader/>
        </instance>

        <collection id="colCntnrFinStmtItm"
                    class="ca.ampautomation.ampata.entity.usr.UsrNode"
                    fetchPlan="fetchPlan_UsrFinStmtItm_Base">
            <loader id="colLoadrFinStmtItm">
                <query>
                    <![CDATA[select e from enty_UsrNode e where e.className = 'UsrFinStmtItm' and e.finStmt1_Id = :finStmt1_Id ORDER BY e.id2]]>
                </query>
            </loader>
        </collection>

    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
        <screenSettings id="settingsFacet" auto="true"/>
    </facets>
    <actions>
        <action id="save" icon="OK" caption="msg:///actions.Ok" shortcut="CTRL-ENTER"/>
        <action id="cancel" icon="CANCEL" caption="msg:///actions.Cancel" description="Esc"/>
    </actions>
    <dialogMode width="1024" height="768"/>
    <layout>
        <split id="split" height="100%" orientation="vertical" width="100%" pos="60" reversePosition="true">
            <vbox id="lookupBox" expand="tableMain" height="100%" margin="false,true,false,false" spacing="true">
                <filter id="filter" dataLoader="colLoadrMain" collapsable="true" collapsed="true" columnsCount="4">
                    <properties include=".*"/>
                    <configurations>
                        <configuration id="filterConfig1" default="true">
                            <propertyFilter id="filterConfig1A_Id2" caption="id2" operation="CONTAINS" property="id2" operationEditable="true"/>
                            <propertyFilter id="filterConfig1A_Desc1" caption="desc1" operation="CONTAINS" property="desc1" operationEditable="true"/>
                            <propertyFilter id="filterConfig1A_IdDtDate1GE" caption="idDt.date1" operation="GREATER_OR_EQUAL" property="nm1s1Inst1Dt1.elDt" operationEditable="true"/>
                            <propertyFilter id="filterConfig1A_IdDtDate1LE" caption="idDt.date1" operation="LESS_OR_EQUAL" property="nm1s1Inst1Dt1.elDt" operationEditable="true"/>
                            <propertyFilter id="filterConfig1A_GenChan1_Id" caption="genChan1" operation="EQUAL" property="genChan1_Id" operationEditable="true">
                                <entityComboBox metaClass="enty_UsrNode" width="200%"/>
                            </propertyFilter>
                            <propertyFilter id="filterConfig1A_FinAcct1_Id" caption="finAcct1" operation="EQUAL" property="finAcct1_Id" operationEditable="true">
                                <entityComboBox metaClass="enty_UsrNode" width="200%"/>
                            </propertyFilter>
                            <propertyFilter id="filterConfig1A_Status" caption="status" operation="CONTAINS" property="status" operationEditable="true"/>
                        </configuration>
                    </configurations>
                </filter>
                <table id="tableMain" width="100%" dataContainer="colCntnrMain" multiselect="true">
                    <actions>
                        <action id="create" type="create"/>
                        <action id="edit" type="edit"/>
                        <action id="refresh" type="refresh"/>
                        <action id="remove" type="remove"/>
                    </actions>
                    <columns>
                        <column id="id2"/>
                        <column id="id2Dup"/>
                        <column id="ts1.elTs" caption="beg1"/>
                        <column id="ts2.elTs" caption="end1"/>
                        <column id="genChan1_Id" caption="genChan1"/>
                        <column id="finAcct1_Id" caption="finAcct1"/>
                        <column id="amtBegBal"><formatter><number format="#,##0.00"/></formatter></column>
                        <column id="amtBegBalCalc"><formatter><number format="#,##0.00"/></formatter></column>
                        <column id="amtDebt"><formatter><number format="#,##0.00"/></formatter></column>
                        <column id="amtCred"><formatter><number format="#,##0.00"/></formatter></column>
                        <column id="amtNet"><formatter><number format="#,##0.00"/></formatter></column>
                        <column id="amtEndBal"><formatter><number format="#,##0.00"/></formatter></column>
