                                            <checkBox id="tmplt_End1Ts1FieldChk" align="MIDDLE_RIGHT"/>
                                        </hbox>
                                        <dateField id="tmplt_End1Ts1Field" datatype="localDateTime" dateFormat="yyyy-MM-dd"/>
                                    </hbox>
                                    <hbox caption="status" width="250px" expand="tmplt_StatusField">
